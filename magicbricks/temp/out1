<script type="text/javascript">
	function show(myref, a)
	{
		var getPost = $(myref).position().left;
		if($('#'+a) != null){
			$('#'+a).css({display:'block'});
			if($(myref).find('#'+a).size() == 0){
				$(myref).find('a').after($('#'+a));
			}
		}
	}
	
	
	function hide(a){
		if( document.getElementById(a) != null){
			document.getElementById(a).style.display="none";
			}
	}
</script>

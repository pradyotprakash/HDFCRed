<script type="text/javascript">
var count=0;

function getData(){
	data=data+"";
	if(data && data.indexOf("HHH") > 0){
		var noOfDiv = data.split("HHH");
		if(count>(noOfDiv.length-2)){
			count=0;
		}
		var divData=noOfDiv[count].split("TTT");
		var h1=String(document.getElementById('headingdiv').innerHTML);
		var h2=String(divData[0]);
		if(ltrim(h1)==ltrim(h2)){
			count++;
			if(count>(noOfDiv.length-2)){
				count=0;
			}
			divData=noOfDiv[count].split("TTT");
		}
		document.getElementById('headingdiv').innerHTML=""+divData[0];
		document.getElementById('disAndUrl').href=divData[1];
		var text="<marquee scrollamount='2' behavior='scroll' loop='1' onmouseover='javascript:stop();' onmouseout='javascript:start();'><a target='_blank' href='"+divData[1]+"' id='disAndUrl'>"+divData[2]+" </a> </marquee>";
		document.getElementById('scroller').innerHTML=text;
		count++;
	}
}

function ltrim(s){
	var l=0;
	while(l < s.length && s[l] == ' '){	
		l++; 
	}
	return s.substring(l, s.length);
}

window.setInterval(function(){
	getData();
}, 40555);
</script>

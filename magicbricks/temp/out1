<script type="text/javascript">
		var comScoreUrlPath = 'http://www.magicbricks.com/bricks/';
		function readCookiemailer(name){
			  var ca = document.cookie.split(';');
			  var nameEQ = name + "=";
			  for(var i=0; i < ca.length; i++) {
			    var c = ca[i];
			    while (c.charAt(0)==' ') c = c.substring(1, c.length); //delete spaces
			    if (c.indexOf(nameEQ) == 0){
			    	retVal=c.substring(nameEQ.length, c.length);
			    	if(name=='userType' && retVal.indexOf(",")>0)
			    		return retVal.substring(0, retVal.indexOf(","));
			    	else if(name=='userEmail'){
				    	return retVal.replace(/^"(.\*)"$/, '$1');
				    }else if(name=='userName'){
				    	return retVal.replace(/^"(.\*)"$/, '$1');
				    }
			    	else
			    		return retVal;
			    }
			   }
			  return null;
		}
		function createCookiemailer(name, value, hour){
			  var expires = "";
			  if(name=='userType' && value.indexOf(",")>0)
			  	value=value.substring(0, value.indexOf(","));
			  if (hour) {
			    var date = new Date();
			    date.setTime(date.getTime()+(hour*60*60*1000));
			    expires ="; expires="+date.toGMTString();
			      if(name == '_no_stay_sign_'){
				  	expires=date.toGMTString();
				  	document.cookie = name + "=" +value + "; expires=" + expires + "; path=/";
				  }
				  else
				  	document.cookie = name+"="+value+expires+"; path=/";
			  }else{
			    	document.cookie = name+"="+value+expires+"; path=/";
			  }
		}
		function createCookiemailerflslider(name, value, hour){
			  var expires = "";
			  if (hour) {
			    var date = new Date();
			    date.setTime(date.getTime()+(hour*60*60*1000));
			    expires ="; expires="+date.toGMTString();
			    document.cookie = name+"="+value+expires+"; domain=.magicbricks.com; path=/";
			  }
		}
		var userType="null";
		var name="null";
		var emailid="null";
		var mobile="null";
		var userMobileCountry="null";
		if(!readCookiemailer("userType")){
			if(userType != null && userType != 'null' && userType != ''){
				createCookiemailer("userType",userType,2);
				createCookiemailerflslider("userTypeflSlider",userType,24);
			}
		}
		if(!readCookiemailer("userName")){
			if(name != null && name != 'null' && name != ''){
				createCookiemailer("userName",name,2);
				createCookiemailerflslider("userNameflSlider",name,24);
			}
		}
		if(!readCookiemailer("userEmail")){
			if(emailid != null && emailid != 'null' && emailid != ''){
				ajaxService.validateAndDecryptEmailId(emailid,{callback:function(str){
	    		emailid = str;
	    		},async:false});
				createCookiemailer("userEmail",emailid,2);
				createCookiemailerflslider("userEmailflSlider",emailid,24);
			}
		}
		if(!readCookiemailer("userMobile")){
			if(mobile != null && mobile != 'null' && mobile != ''){
				ajaxService.validateAndDecryptMobile(mobile,{callback:function(str){
	    		mobile = str;
	    		},async:false});
				createCookiemailer("userMobile",mobile,2);
				createCookiemailerflslider("userMobileflSlider",mobile,24);
			}
		}
		if(!readCookiemailer("userMobileCountry")){
			if(userMobileCountry != null && userMobileCountry != 'null' && userMobileCountry != ''){
				createCookiemailer("userMobileCountry",userMobileCountry,2);
				createCookiemailerflslider("userMobileCountryflSlider",userMobileCountry,24);
			}
		}
	</script>

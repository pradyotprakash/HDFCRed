<script type="text/javascript">
function showTabb(showDiv,hideDiv){
		document.getElementById('tabb'+showDiv).style.display='';
		for(var i = 1; i <= hideDiv; i++){
			if(document.getElementById('tabb'+i) && i != showDiv){
				document.getElementById('tabb'+i).style.display='none';
				document.getElementById(i+'_link').style = '';
				document.getElementById(i+'_li').className = '';
			}else if(i == showDiv){
				document.getElementById(showDiv+'_li').className = 'currentLink-bottom';
				if(document.getElementById(showDiv+'_li').className != 'currentLink-bottom'){
					track_comScore('seo_text_tabs_in_footer');
				}
			}
		}
	}
</script>

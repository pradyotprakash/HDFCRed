<script language="javascript" type="text/javascript">
function validateEmail(email){
	var atpos = email.indexOf("@");
    var dotpos = email.lastIndexOf(".");
    if (atpos<1 || dotpos<atpos+2 || dotpos+2>=email.length) {
        return false;
    }else{
     return true;
    }
}



function saveEmailWithSearchInfoInAlert(){

 $("#invalidEmailAlert").hide();
 $("#blankEmailAlert").hide();
 $("#yourEmailBoxAlert").removeClass("borderErrRed");
 var urlData=getUrlStringToSearch();
 var emailId = $.trim($('#yourEmailBoxAlert').val());
	if(emailId.length!= 0){
	 if (validateEmail(emailId)) {
	 var allcookies = document.cookie;
	 var context= '/bricks';
	 urlData= urlData.replace(new RegExp('&', 'g'), '&&');
	 var userName= getCookie('userName');
	 var userMobile= getCookie('userMobile');
	 var userMobileCountry=getCookie('userMobileCountry');
	 var userType= getCookie('userType');
	if(typeof userName=== 'undefined'){
			 userName="";
		 }
		 if(typeof userMobile=== 'undefined'){
			 userMobile="";
		 }
		 if(typeof userMobileCountry=== 'undefined'){
			 userMobileCountry="";
		 }
		 if(typeof userType==='undefined'){
			 userType="";
		 }
   	saveAlertForm(context, emailId, urlData, userName, userMobile, userMobileCountry, userType,'Y');
   
    }else{
  $("#invalidEmailAlert").show();
  $("#yourEmailBoxAlert").addClass("borderErrRed");
    }	
}else{
$("#blankEmailAlert").show();
$("#yourEmailBoxAlert").addClass("borderErrRed");
}
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) != -1) return c.substring(name.length,c.length);
    }
    return "";
} 

</script>

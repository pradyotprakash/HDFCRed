<script type="text/javascript">
var eroi= getSrpEmiCookie('srpEmiRoi');
if(eroi == null || eroi == ""){
	eroi = srpEmiRoi;
}

var eTenure = getSrpEmiCookie('srpEmiTenure');
if(eTenure == null || eTenure == ""){
	eTenure = srpEmiTenure;
}
eTenure = eTenure *12;
eroi = eroi / 1200;
var annuity = (1 - (Math.pow(1/(1 + eroi), eTenure)));
$('a.emiSRPCalc input.srpEmiPropPriceInput').each( function( index, element ){
	calcSrpEmi(eroi, annuity, this, $(element).val());
});

</script>

		ajaxService.loadPostRequirement(reqObject, {callback:function(data){
			if(data!=null){
				$("#searchResultLeaderBoard").html(data);
				if ($.browser.msie && $.browser.version <= 6 ){
		      	}else{
		      	  var count=document.getElementById("resultCount").innerHTML;
		          if(count == 0 || count == '0'){
				 		document.getElementById("fred").innerHTML = "";
				 	}else{
				 		lbComScoreGafire();
				 		if(readCookie("_LB_SHOWN")==null || readCookie("_LB_SHOWN") != 'Y' )  {
				 			createCookie("_LB_SHOWN", "Y", 1);
					 		$(window).scroll(function(){
							    if(currentYPosition()>=500 && !leaderboardCallStatus){
							    	$("#fred").show();
									$("#leaderBoard").show(); 
					 		    	leaderboardCallStatus=true;
							    }
					 		});
				 		}
				 	}
		        }
			}
		}, async:true});
	}
		
	function currentYPosition() {
	 	var scrollPos = 0;
	    // Firefox, Chrome, Opera, Safari
	    if (self.pageYOffset){
	    	scrollPos = self.pageYOffset;
	    }
	    // Internet Explorer 6 - standards mode
	    if (document.documentElement && document.documentElement.scrollTop){
	    	scrollPos = document.documentElement.scrollTop;
	    }
	    // Internet Explorer 6, 7 and 8
	    if (document.body.scrollTop){
	    	scrollPos = document.body.scrollTop;
	    }
	    return scrollPos;
	}	
	function lbComScoreGafire() {
		 // $("#fred").show();
		 if($("#leaderBoard"))
		 	$("#leaderBoard").show();
		 if($("#projectLBpopup"))
		 	$("#projectLBpopup").show();
		 track_comScore('PropertySRP_leaderboard');
		 if(readCookie("SHOWSEARCHTEXT") == ''){
			var eventType7 = '';
			createCookie("defaultLeaderBoard", "D", 1);
			//fire default open event for property, project and agent search
			
				eventType7 = 'Default_LB_Property_RHS_Maximize';
			
			
			
			if(eventType7 != ''){
				_gaq.push(['_trackEvent', 'Leaderboard Post Requirement', eventType7]);
			}
		}
	}
